{
  "name": "@augmentos/debug-audio",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "dev": "bun run dev:concurrent",
    "dev:server": "bun --hot src/app.ts",
    "dev:webview": "cd webview && bun run dev",
    "dev:concurrent": "bun run clean-wav-files && bun run dev:webview & bun run dev:server",
    "dev:build": "cd webview && bun run build && cd .. && bun --hot server/src/app.ts",
    "build": "cd webview && bun run build && cd .. && bun build ./server/src/app.ts --outdir ./dist --target bun",
    "start": "bun dist/app.js",
    "clean-wav-files": "bun run server/src/scripts/clean-wav-files.ts",
    "docker:dev": "docker-compose -f docker/docker-compose.dev.yml -p dev up",
    "docker:dev:detach": "docker-compose -f docker/docker-compose.dev.yml -p dev up -d",
    "docker:stop": "docker-compose -f docker/docker-compose.dev.yml -p dev down",
    "docker:build": "docker-compose -f docker/docker-compose.dev.yml -p dev build",
    "ngrok": "ngrok http --url=isaiah-tpa.ngrok.app 8069",
    "add": "docker exec -it debug-audio-dev bun add",
    "add:dev": "docker exec -it debug-audio-dev bun add -d",
    "remove": "docker exec -it debug-audio-dev bun remove",
    "test": "docker exec -it debug-audio-dev bun test",
    "lint": "docker exec -it debug-audio-dev npx eslint 'src/**/*.ts'",
    "logs": "docker logs -f debug-audio-dev",
    "restart": "docker restart debug-audio-dev",
    "exec": "docker exec -it debug-audio-dev",
    "sh": "docker exec -it debug-audio-dev bash",
    "prod": "docker-compose -f docker/docker-compose.yml up",
    "prod:detach": "docker-compose -f docker/docker-compose.yml up -d",
    "prod:stop": "docker-compose -f docker/docker-compose.yml down",
    "prod:build": "docker-compose -f docker/docker-compose.yml build",
    "image:build": "docker build -t augmentos/debug-audio:latest -t augmentos/debug-audio:$npm_package_version -f docker/Dockerfile .",
    "image:push": "docker push augmentos/debug-audio:latest && docker push augmentos/debug-audio:$npm_package_version",
    "image:release": "npm run image:build && npm run image:push",
    "ghcr:build": "docker build -t ghcr.io/augmentos/debug-audio:latest -t ghcr.io/augmentos/debug-audio:$npm_package_version -f docker/Dockerfile .",
    "ghcr:push": "docker push ghcr.io/augmentos/debug-audio:latest && docker push ghcr.io/augmentos/debug-audio:$npm_package_version",
    "ghcr:release": "npm run ghcr:build && npm run ghcr:push"
  },
  "dependencies": {
    "@augmentos/sdk": "^1.1.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.1",
    "@types/node": "^22.13.14",
    "@types/ws": "^8.18.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "http-proxy": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "path": "^0.12.7",
    "ws": "^8.18.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}